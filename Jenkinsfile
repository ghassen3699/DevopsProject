pipeline{
    agent any
    stages{
      stage('build image'){
            steps{
                script{
                    echo "build image"
                }
            }
        }
      stage('test project'){
            steps{
                script{
                    echo "test project"
                }
            }
        }
        stage('deploy image'){
            steps{
                script{
                    echo "deploy image"
                }
            }
        }
    }
}
